{
  "name": "cdk-infra-controller",
  "version": "0.1.0",
  "bin": {
    "cdk-infra-controller": "bin/cdk-infra-controller.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "lint": "eslint .",
    "test:dev:config": "jest -- test/dev.test.ts",
    "test:prod:config": "jest -- test/prod.test.ts",
    "test:unit:cloudfront": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage -- test/constructs/cloudfront-construct.test.ts",
    "test:unit:route53": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage -- test/constructs/route53-construct.test.ts",
    "test:constructs": "node --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage -- test/cdk-aws-apps-controller-stack.test.ts",
    "diff:dev": "cdk diff --profile thwaitehowe",
    "diff:prod": "cdk diff --app \"npx ts-node bin/prod.ts\" --profile thwaitehowe",
    "cdk:dev": "cdk synth --profile thwaitehowe",
    "cdk:prod": "cdk synth --app \"npx ts-node bin/prod.ts\" --profile thwaitehowe",
    "deploy:dev": "cdk deploy --profile thwaitehowe",
    "deploy:prod": "cdk deploy --app \"npx ts-node bin/prod.ts\" --profile thwaitehowe"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.134.0",
    "aws-sdk": "^2.1692.0",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.134.0",
    "constructs": "^10.0.0"
  }
}
